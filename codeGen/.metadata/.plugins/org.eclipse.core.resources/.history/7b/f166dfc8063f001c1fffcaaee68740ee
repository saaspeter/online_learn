    private Map getpaperWareIDExampleParms(PaperwareExample example) {
        Map parms = new HashMap();
        switch (example.getPaperwareid_Indicator()) {
        case PaperwareExample.EXAMPLE_NULL:
            if (example.isCombineTypeOr()) {
                parms.put("OR_paperWareID_NULL", "Y");
            } else {
                parms.put("AND_paperWareID_NULL", "Y");
            }
            break;
        case PaperwareExample.EXAMPLE_NOT_NULL:
            if (example.isCombineTypeOr()) {
                parms.put("OR_paperWareID_NOT_NULL", "Y");
            } else {
                parms.put("AND_paperWareID_NOT_NULL", "Y");
            }
            break;
        case PaperwareExample.EXAMPLE_EQUALS:
            if (example.isCombineTypeOr()) {
                parms.put("OR_paperWareID_EQUALS", "Y");
            } else {
                parms.put("AND_paperWareID_EQUALS", "Y");
            }
            parms.put("paperwareid", example.getPaperwareid());
            break;
        case PaperwareExample.EXAMPLE_NOT_EQUALS:
            if (example.isCombineTypeOr()) {
                parms.put("OR_paperWareID_NOT_EQUALS", "Y");
            } else {
                parms.put("AND_paperWareID_NOT_EQUALS", "Y");
            }
            parms.put("paperwareid", example.getPaperwareid());
            break;
        case PaperwareExample.EXAMPLE_GREATER_THAN:
            if (example.isCombineTypeOr()) {
                parms.put("OR_paperWareID_GT", "Y");
            } else {
                parms.put("AND_paperWareID_GT", "Y");
            }
            parms.put("paperwareid", example.getPaperwareid());
            break;
        case PaperwareExample.EXAMPLE_GREATER_THAN_OR_EQUAL:
            if (example.isCombineTypeOr()) {
                parms.put("OR_paperWareID_GE", "Y");
            } else {
                parms.put("AND_paperWareID_GE", "Y");
            }
            parms.put("paperwareid", example.getPaperwareid());
            break;
        case PaperwareExample.EXAMPLE_LESS_THAN:
            if (example.isCombineTypeOr()) {
                parms.put("OR_paperWareID_LT", "Y");
            } else {
                parms.put("AND_paperWareID_LT", "Y");
