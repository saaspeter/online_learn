      <isNotNull prepend="," property="productname" >
        productName = #productname:VARCHAR#
      </isNotNull>
      <isNotNull prepend="," property="categoryid" >
        categoryID = #categoryid:BIGINT#
      </isNotNull>
      <isNotNull prepend="," property="productcost" >
        productCost = #productcost:DOUBLE#
      </isNotNull>
      <isNotNull prepend="," property="cantry" >
        canTry = #cantry:SMALLINT#
      </isNotNull>
      <isNotNull prepend="," property="trydatenum" >
        tryDateNum = #trydatenum:INTEGER#
      </isNotNull>
      <isNotNull prepend="," property="status" >
        status = #status:SMALLINT#
      </isNotNull>
      <isNotNull prepend="," property="productdesc" >
        productDesc = #productdesc:LONGVARCHAR#
      </isNotNull>
    </dynamic>
    where productID = #productid#
  </update>
</sqlMap>