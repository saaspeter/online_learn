    private Map getshopConfigIDExampleParms(ShopconfigExample example) {
        Map parms = new HashMap();
        switch (example.getShopconfigid_Indicator()) {
        case ShopconfigExample.EXAMPLE_NULL:
            if (example.isCombineTypeOr()) {
                parms.put("OR_shopConfigID_NULL", "Y");
            } else {
                parms.put("AND_shopConfigID_NULL", "Y");
            }
            break;
        case ShopconfigExample.EXAMPLE_NOT_NULL:
            if (example.isCombineTypeOr()) {
                parms.put("OR_shopConfigID_NOT_NULL", "Y");
            } else {
                parms.put("AND_shopConfigID_NOT_NULL", "Y");
            }
            break;
        case ShopconfigExample.EXAMPLE_EQUALS:
            if (example.isCombineTypeOr()) {
                parms.put("OR_shopConfigID_EQUALS", "Y");
            } else {
                parms.put("AND_shopConfigID_EQUALS", "Y");
            }
            parms.put("shopconfigid", example.getShopconfigid());
            break;
        case ShopconfigExample.EXAMPLE_NOT_EQUALS:
            if (example.isCombineTypeOr()) {
                parms.put("OR_shopConfigID_NOT_EQUALS", "Y");
            } else {
                parms.put("AND_shopConfigID_NOT_EQUALS", "Y");
            }
            parms.put("shopconfigid", example.getShopconfigid());
            break;
        case ShopconfigExample.EXAMPLE_GREATER_THAN:
            if (example.isCombineTypeOr()) {
                parms.put("OR_shopConfigID_GT", "Y");
            } else {
                parms.put("AND_shopConfigID_GT", "Y");
            }
            parms.put("shopconfigid", example.getShopconfigid());
            break;
        case ShopconfigExample.EXAMPLE_GREATER_THAN_OR_EQUAL:
