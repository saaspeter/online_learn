        Map parms = new HashMap();
        parms.putAll(getrechargeIDExampleParms(example));
        parms.putAll(getuserIDExampleParms(example));
        parms.putAll(get充值时间ExampleParms(example));
        parms.putAll(get充值金额ExampleParms(example));
        parms.putAll(get币种ExampleParms(example));
        parms.putAll(get充值点数ExampleParms(example));
        parms.putAll(get余额ExampleParms(example));
        return parms;
    }
}